{
  "name": "svelte5-vite-ts-tailwind-eslint",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --check vite.config.ts src && eslint vite.config.ts src && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write vite.config.ts src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.10.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.5.1",
    "flowbite-svelte": "^1.5.3",
    "globals": "^16.0.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.31.1",
    "vite": "^7.0.5",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@felte/validator-yup": "^1.1.4",
    "bmlt-server-client": "^2.0.0",
    "felte": "^1.3.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yup": "^1.6.1"
  }
}
